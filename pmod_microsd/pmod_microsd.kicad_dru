(version 1)
(rule "USON neckdown U1"
    (constraint track_width (min 5mil) (opt 10mil))
    (constraint clearance (min 5mil) (opt 6mil))
    (condition "A.intersectsFrontCourtyard('U1')"))
(rule "USON neckdown U2"
    (constraint track_width (min 5mil) (opt 10mil))
    (constraint clearance (min 5mil) (opt 6mil))
    (condition "A.intersectsFrontCourtyard('U2')"))
